b
  font-weight bold

i
  font-style italic

.aas-logo img
  display block

table
  display table
  border-collapse collapse
  border 0
  box-sizing border-box
  text-indent initial

table

  &.table-800
    width 800px

  &.table-100
    width 100%

  &.mb-8
    margin-bottom 8px

  &.mb-16
    margin-bottom 16px

  &.mb-32
    margin-bottom 32px

  &.mb-30
    margin-bottom 30px

  &.mb-100
    margin-bottom 100px

td
  font-family "Roboto", Arial, sans-serif
  vertical-align top
  color #000
  font-weight normal
  font-size 10px
  line-height normal
  vertical-align top

  &.text-right
    text-align right

  &.text-center
    text-align center

  &.text-bottom
    vertical-align bottom

  &.nowrap
    white-space nowrap

  &.pb-4
    padding-bottom 4px

  &.pb-16
    padding-bottom 16px

  &.pb-32
    padding-bottom 32px

td.border
  border-bottom 1px solid #000
  padding-top 5px

table.header

  td
    font-size 8px

table.h1

  td
    font-size 18px
    font-weight bold

table.items

  td
    text-align left

table.stamp
  border 3px solid #173457
  min-width 220px
  margin-top 50px
  width auto

  td
    text-align center
    text-transform uppercase
    font-family "Roboto Condensed", Roboto, Arial, sans-serif
    font-weight bold
    color #0A355A
    padding 0 15px

  td.pt7
    padding-top 7px

  td.pb7
    padding-bottom 7px

  td.fs19
    font-size 19px

  td.fs11
    font-size 11px

  td.fs10
    font-size 10px

  td.fs8
    font-size 8px